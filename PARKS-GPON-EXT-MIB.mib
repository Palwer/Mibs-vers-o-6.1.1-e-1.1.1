	}
	
	onuVeipSlotNo OBJECT-TYPE	
		SYNTAX		SlotNo
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Serves as an index for this table."
		::=  {  onuVeipEntry  1  }  

	onuVeipPortNo OBJECT-TYPE	
		SYNTAX		PortNo
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Serves as an index for this table."
