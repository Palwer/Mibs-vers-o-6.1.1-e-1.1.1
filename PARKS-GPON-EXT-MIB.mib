	OnuTr069ProfEntry ::= SEQUENCE {
		onuTr069ProfIndex Unsigned32,
		onuTr069ProfName ProfileName,
		onuTr069ProfAcsUrl OCTET STRING,
		onuTr069ProfUserType INTEGER,
		onuTr069ProfUsername OCTET STRING,
		onuTr069ProfPassword OCTET STRING,
		onuTr069ProfValidationScheme INTEGER,
		onuTr069ProfRealm OCTET STRING,
		onuTr069ProfVlanMode INTEGER,
		onuTr069ProfVlanId Unsigned32,
		onuTr069ProfVlanPriority Unsigned32,
		onuTr069ProfRowStatus RowStatus
	}

	onuTr069ProfIndex OBJECT-TYPE
		SYNTAX          Unsigned32(2..1025)
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "Serves as an index for this table."
		::=  { onuTr069ProfEntry 1 }

	onuTr069ProfName OBJECT-TYPE
		SYNTAX          ProfileName
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION     "Name of TR-069 profile"
		::=  { onuTr069ProfEntry  2 }

	onuTr069ProfAcsUrl OBJECT-TYPE
		SYNTAX          OCTET STRING (SIZE(0..150))
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION     "ACS URL"
		::=  { onuTr069ProfEntry 3 }

	onuTr069ProfUserType OBJECT-TYPE
		SYNTAX          INTEGER { none(0), serial-number(1), alias(2), text(3) }
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION     "Username for ACS connection
		                0 - (none) No user authentication
		                1 - (serial-number) Uses the ONU serial-number
		                2 - (alias) Uses the ONU alias
		                3 - (text) Uses username oid"
		DEFVAL          { 1 }
		::=  { onuTr069ProfEntry 4 }

	onuTr069ProfUsername OBJECT-TYPE
		SYNTAX          OCTET STRING (SIZE(0..50))
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION     "Username for ACS connection if user type is text"
		::=  { onuTr069ProfEntry 5 }

	onuTr069ProfPassword OBJECT-TYPE
		SYNTAX          OCTET STRING (SIZE(0..25))
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION     "Password for ACS connection"
		::=  { onuTr069ProfEntry 6 }

	onuTr069ProfValidationScheme OBJECT-TYPE
		SYNTAX          INTEGER { disable(0), md5(1), basic(2) }
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION     "ACS connection validation scheme.
		                0 - disable
		                1 - md5
		                2 - basic"
		DEFVAL          { 0 }
		::=  { onuTr069ProfEntry 7 }

	onuTr069ProfRealm OBJECT-TYPE
		SYNTAX          OCTET STRING (SIZE(0..25))
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION     "Realm used in validation"
		::=  { onuTr069ProfEntry 8 }


	onuTr069ProfVlanMode OBJECT-TYPE
		SYNTAX          INTEGER { untagged(0), tagged(1) }
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION     "Vlan mode to management TR-069 traffic passing through the VEIP.
		                0 - Untagged
		                1 - Tagged"
		DEFVAL          { 0 }
		::=  { onuTr069ProfEntry 9 }

	onuTr069ProfVlanId OBJECT-TYPE
		SYNTAX          Unsigned32(0..4095)
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION     "If vlan mode is tagged, use this field to set vlan id"
		::=  { onuTr069ProfEntry 10 }

	onuTr069ProfVlanPriority OBJECT-TYPE
		SYNTAX          Unsigned32(0..7)
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION     "If vlan mode is tagged, use this field to set vlan priority"
		::=  { onuTr069ProfEntry 11 }

	onuTr069ProfRowStatus OBJECT-TYPE
		SYNTAX          RowStatus
		MAX-ACCESS      read-create
		STATUS          current
		DESCRIPTION     "For row creation/deletion."
		::=  {  onuTr069ProfEntry  12 }

