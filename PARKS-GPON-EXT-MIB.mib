--OnuMulticastOperProfExtTable--
	onuMulticastOperProfExtTable OBJECT-TYPE
		SYNTAX		SEQUENCE  OF  OnuMulticastOperProfExtEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Parks GPON Multicast Opereration Profile Table Extension "
		::=  { gponOnuMulticastOperProfExtTable 1 }

	onuMulticastOperProfExtEntry OBJECT-TYPE
		SYNTAX		OnuMulticastOperProfExtEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Row Description"
		INDEX		{ onuMulticastOperProfileIndex }
		::=  { onuMulticastOperProfExtTable 1 }

	OnuMulticastOperProfExtEntry  ::=  SEQUENCE {
		onuMulticastOperProfileIndex	Unsigned32,
		onuMulticastOperProfUnauthorizedJoinRequest	INTEGER,
		onuMulticastOperProfDynamicAccessControl	INTEGER,
		onuMulticastOperProfDownstreamIGMPTCI Unsigned32,
		onuMulticastOperProfDownstreamIGMPTagControl Unsigned32
	}

	onuMulticastOperProfileIndex 	OBJECT-TYPE
		SYNTAX		Unsigned32(2..1280)
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Index for this table."
		::=  {  onuMulticastOperProfExtEntry  1 }

	onuMulticastOperProfUnauthorizedJoinRequest	OBJECT-TYPE
		SYNTAX		INTEGER { disable(0), enable(1) }
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"This attribute specifies the ONU's behaviour when it 
				   receives an IGMP join request for a group that is not 
				   authorized in the dynamic address control list table:
					0 Disabled
					1 Enabled"
		DEFVAL			{ 0 }
		::=  {  onuMulticastOperProfExtEntry  2  }

	onuMulticastOperProfDynamicAccessControl	OBJECT-TYPE
		SYNTAX		INTEGER { disable(0), enable(1) }
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"This attribute specifies the ONU's behaviour in relation to 
				   configuration the access control list mode. If enable, 
				   mode is dynamic and if disable (default) mode is static:
					0 Disabled
					1 Enabled"
		DEFVAL			{ 0 }
		::=  {  onuMulticastOperProfExtEntry  3  }

	onuMulticastOperProfDownstreamIGMPTCI 	OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION	"Under control of the downstream IGMP tag control attribute, the downstream IGMP TCI attribute 
						defines a VLAN ID and P bits to add to downstream IGMP messages:
            ?	The multicast VLAN associated with the multicast group according to ONU multicast group and 
            VLAN profile if multicastControlMode = 4 CTC distributed mode
            ?	UNI port index (starting from 1) if multicastControlMode = 5 CTC controllable mode
            ?	Use as 0 if multicastControlMode is neither 4 nor 5
            Other values Use as is."
		DEFVAL			{ 0 }
		::=  {  onuMulticastOperProfExtEntry  4 }
		
	onuMulticastOperProfDownstreamIGMPTagControl 	OBJECT-TYPE
		SYNTAX		Unsigned32(0..4)
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION	"This attribute controls the downstream IGMP TCI attribute:
            0 Pass downstream IGMP and multicast traffic transparently, neither stripping nor modifying tags that may be present.
	    1 Strip the outer VLAN tag (including P bits) from the downstream IGMP and multicast traffic.
	    2 Add a tag on to the downstream IGMP/MLD and multicast traffic. The new tag TCI is specified by the downstream IGMP TCI attribute.
            3 Replace the tag on downstream IGMP and multicast traffic. The new tag TCI is specified by the downstream IGMP TCI attribute.
            4 Replace only the VLAN ID on downstream IGMP and multicast traffic, retaining the original CFI and P bits. The new VLAN ID is specified by
            the VLAN field of the downstream IGMP TCI attribute."
	    	DEFVAL			{ 0 }
		::=  {  onuMulticastOperProfExtEntry  5 }
END --PARKS-EXT-MIB
