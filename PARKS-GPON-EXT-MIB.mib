		onuVeipStaticMulticastGroupsProfileIndex Unsigned32,
		onuVeipMACFilterProfileIndex Unsigned32,
		onuVeipMACFilterPreassignProfileIndex Unsigned32,		
		onuVeipPppoeIndex Unsigned32,
