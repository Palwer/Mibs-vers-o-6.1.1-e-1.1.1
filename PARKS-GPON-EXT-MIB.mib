--    DESCRIPTION
--	"MIB with configuration for GPON in PARKS devices. "

PARKS-GPON-EXT-MIB DEFINITIONS ::= BEGIN
    IMPORTS
	OBJECT-TYPE, Unsigned32
		FROM SNMPv2-SMI
	onuVPortSvcProfileIndex
		FROM GPON-OLT-SVC
	parksGponExt
		FROM PARKS-FEATURES-MIB
	RowStatus,TruthValue
		FROM SNMPv2-TC
	VirtualPortServiceProfileNo,SchedulerProfileNo,SlotNo,PortNo,LogicalPortNo,MacAddress,PhysicalPortNo,ProfileName
		FROM GPON-TC
	Ipv6Address,Ipv6AddressPrefix
		FROM IPV6-TC
	SystemModule, SysMsgDescription
		FROM GPON-OLT-TC
	ProtectionType
		FROM GPON-TC;

	gponOnuVPortSvcProfExtTable      OBJECT IDENTIFIER ::= { parksGponExt 1 }
	gponOnuBlackListExtTable         OBJECT IDENTIFIER ::= { parksGponExt 2 }
	gponOnuIpv6HostTable             OBJECT IDENTIFIER ::= { parksGponExt 3 }
	gponOltSystemMessages            OBJECT IDENTIFIER ::= { parksGponExt 4 }
	gponOltSystemDictionary          OBJECT IDENTIFIER ::= { parksGponExt 5 }
	gponOnuDot1xTable                OBJECT IDENTIFIER ::= { parksGponExt 6 }
	gponOnuCfgExtTable               OBJECT IDENTIFIER ::= { parksGponExt 7 }
	gponOnuAlarmIndication           OBJECT IDENTIFIER ::= { parksGponExt 8 }
	gponOltPonLinkCfgExtTable        OBJECT IDENTIFIER ::= { parksGponExt 9 }
	gponOnuCfgResetConfigExtTable    OBJECT IDENTIFIER ::= { parksGponExt 10 }
	gponOltSysCfg                    OBJECT IDENTIFIER ::= { parksGponExt 11 }
	gponOltProtectionLinkSetExtTable OBJECT IDENTIFIER ::= { parksGponExt 12 }
	gponOnuIpHostTable               OBJECT IDENTIFIER ::= { parksGponExt 13 }
	gponOnuVeipTable                 OBJECT IDENTIFIER ::= { parksGponExt 14 }
	gponOnuPppoeConfigTable          OBJECT IDENTIFIER ::= { parksGponExt 15 }
	gponOnuTr069ProfTable            OBJECT IDENTIFIER ::= { parksGponExt 16 }
	gponOnuTr069ConfigTable          OBJECT IDENTIFIER ::= { parksGponExt 17 }
	gponOnuMulticastOperProfExtTable OBJECT IDENTIFIER ::= { parksGponExt 18 }

--==========================================================================
	onuVPortSvcProfExtTable 	OBJECT-TYPE
		SYNTAX		SEQUENCE  OF  OnuVPortSvcProfExtEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	  "Parks GPON OnuVPortSvcProfTable Extension "
		::=  { gponOnuVPortSvcProfExtTable  1 }

	onuVPortSvcProfExtEntry	OBJECT-TYPE
		SYNTAX		OnuVPortSvcProfExtEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Row Description"
		INDEX		{ onuVPortSvcProfileIndex }
		::=  { onuVPortSvcProfExtTable 1 }
		
	OnuVPortSvcProfExtEntry  ::=  SEQUENCE {
		onuVPortSvcProfileIndex Unsigned32,
		onuVPortSvcProfUSRateControlSchedulerProfileIndex Unsigned32,
		onuVPortSvcProfUpstreamPriorityQueueProfileIndex Unsigned32,
		onuVPortSvcProfDownstreamPriorityQueueProfileIndex Unsigned32,
		onuVportSvcProfUpstreamQosMethod INTEGER
	}

	onuVPortSvcProfileIndex	OBJECT-TYPE	
		SYNTAX			VirtualPortServiceProfileNo
		MAX-ACCESS		not-accessible
		STATUS			current
		DESCRIPTION		"Serves as an index for this table.2-7680"
		::=  { onuVPortSvcProfExtEntry  1 }
		
	onuVPortSvcProfUSRateControlSchedulerProfileIndex	OBJECT-TYPE	
		SYNTAX			SchedulerProfileNo
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION	  "It points to the associated ONU upstream rate-control scheduler profile.0 - 7680
				   Set to 0 to remove profile configuration."
		DEFVAL			{ 1 }
		::=  {  onuVPortSvcProfExtEntry  2  }
		
	onuVPortSvcProfUpstreamPriorityQueueProfileIndex	OBJECT-TYPE	
		SYNTAX			Unsigned32(1..128)
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION	  "It points to the associated ONU upstream priority queue profile.1 - 128"
		DEFVAL			{ 1 }
		::=  {  onuVPortSvcProfExtEntry  3  }

	onuVPortSvcProfDownstreamPriorityQueueProfileIndex	OBJECT-TYPE	
		SYNTAX			Unsigned32(1..128)
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION	  "It points to the associated ONU downstream priority queue profile.1 - 128"
		DEFVAL			{ 1 }
		::=  {  onuVPortSvcProfExtEntry  4  }

	onuVportSvcProfUpstreamQosMethod	OBJECT-TYPE	
		SYNTAX			INTEGER{flow-based(1), tci-based(2)}
		MAX-ACCESS		read-write
		STATUS			current
		DESCRIPTION	  "Upstream QoS Method
						1- flow-based
						2- tci-based "
		DEFVAL			{ 1 }
		::=  {  onuVPortSvcProfExtEntry  5  }

--==========================================================================

--ONU Black List NG Table--

	onuBlackListExtTable OBJECT-TYPE
		SYNTAX		SEQUENCE  OF  OnuBlackListExtEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Parks GPON onuBlackListTable Extension "
		::=  { gponOnuBlackListExtTable 1 }

	onuBlackListExtEntry OBJECT-TYPE
		SYNTAX		OnuBlackListExtEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Row Description"
		INDEX		{ onuBlackListSerialNumber }
		::=  { onuBlackListExtTable 1 }

	OnuBlackListExtEntry  ::=  SEQUENCE {
		onuBlackListSerialNumber	OCTET STRING,
		onuBlackListRowStatus		RowStatus
	}

	onuBlackListSerialNumber	OBJECT-TYPE
		SYNTAX		OCTET STRING(SIZE(16))
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"The serial number. Index for this table.
				 The serial number is unique for each ONT.

				 It is defined in ITU-T G.984.3 and contains
				  the vendor id and version number. The first
				  four bytes are an ASCII encoded vendor ID
				  four letter mnemonic. The second four bytes
				  are a binary encoded serial number, under the
				  control of the ONT vendor. (R) (mandatory) (8 bytes).
				 When a ONT added, all it's configurations are removed."
		::=  {  onuBlackListExtEntry  1  }

	onuBlackListRowStatus	OBJECT-TYPE
		SYNTAX		RowStatus
                MAX-ACCESS	read-write
                STATUS		current
		DESCRIPTION	"For row creation/deletion."
		::=  {  onuBlackListExtEntry  2  }

--==========================================================================
--  ONU IPV6 HOST TABLE --
	onuIpv6HostTable OBJECT-TYPE
		SYNTAX		SEQUENCE  OF  OnuIpv6HostEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	  "Parks GPON Ipv6 Host Table"
		::=  { gponOnuIpv6HostTable  1 }

	onuIpv6HostEntry OBJECT-TYPE
		SYNTAX		OnuIpv6HostEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Row Description"
		INDEX		{   onuIpv6HostSlotNo, onuIpv6HostPortNo, onuIpv6HostLogicalPortNo }
		::=  { onuIpv6HostTable 1 }

	OnuIpv6HostEntry ::= SEQUENCE {
		onuIpv6HostSlotNo SlotNo,
		onuIpv6HostPortNo PortNo,
		onuIpv6HostLogicalPortNo LogicalPortNo,
		onuIpv6HostAdministrativeState INTEGER,
		onuIpv6HostRSMode TruthValue,
		onuIpv6HostDHCPv6Mode TruthValue,
		onuIpv6HostRespond2PingsMode TruthValue,
		onuIpv6HostOnuID OCTET STRING,
		onuIpv6HostIpv6Address Ipv6Address,
		onuIpv6HostDefaultRouter Ipv6Address,
		onuIpv6HostPrimaryDNS OCTET STRING,
		onuIpv6HostSecondaryDNS OCTET STRING,
		onuIpv6HostOnLinkPrefix Ipv6Address,
		onuIpv6HostOnLinkPrefixLength Unsigned32,
		onuIpv6HostMacAddress MacAddress,
		onuIpv6HostIpv6LinkLocalAddress Ipv6Address,
		onuIpv6HostDUID OCTET STRING,
		onuIpv6HostRowStatus RowStatus
	}

	onuIpv6HostSlotNo OBJECT-TYPE	
		SYNTAX		SlotNo
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Serves as an index for this table."
		::=  {  onuIpv6HostEntry  1  }  

	onuIpv6HostPortNo OBJECT-TYPE	
		SYNTAX		PortNo
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Serves as an index for this table."
		::=  {  onuIpv6HostEntry  2  }  
		
	onuIpv6HostLogicalPortNo OBJECT-TYPE	
		SYNTAX		LogicalPortNo
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Serves as an index for this table. 
                                         It should be identical to ONU ID assigned by OLT. "
		::=  {  onuIpv6HostEntry  3  } 

	onuIpv6HostAdministrativeState OBJECT-TYPE	
		SYNTAX		INTEGER {lock(0), unlock(1)}
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"IPv6 stack administrative unlock: 
				 0- Lock
                                         1- Unlock"
		DEFVAL		{lock}
		::=  {  onuIpv6HostEntry  4  } 

	onuIpv6HostRSMode OBJECT-TYPE	
		SYNTAX		TruthValue
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Enable RS. The host generates router solicitation (RS)  
				messages, if necessary, and responds to router advertisements."
		DEFVAL		{ false }
		::=  {  onuIpv6HostEntry  5  } 

	onuIpv6HostDHCPv6Mode OBJECT-TYPE	
		SYNTAX		TruthValue
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Enable DHCPv6."
		DEFVAL		{ false }
		::=  {  onuIpv6HostEntry  6  } 

	onuIpv6HostRespond2PingsMode OBJECT-TYPE	
		SYNTAX		TruthValue
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Respond to pings (ICMPv6 echo replies)."
		DEFVAL		{ false }
		::=  {  onuIpv6HostEntry  7  } 

	onuIpv6HostOnuID OBJECT-TYPE	
		SYNTAX		OCTET STRING(SIZE(25))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"A unique ONU identifier string. 
                                         If set to a non-null value, his string is used instead of 
                                         the MAC address in retrieving DHCPv6 parameters."
		::=  {  onuIpv6HostEntry  8  } 

	onuIpv6HostIpv6Address OBJECT-TYPE	
		SYNTAX		Ipv6Address
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"The manually provisioned IPv6 address used for routed IPv6 host services."
		::=  {  onuIpv6HostEntry  9  } 

	onuIpv6HostDefaultRouter OBJECT-TYPE	
		SYNTAX		Ipv6Address
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"The manually provisioned IPv6 address of the default router."
		::=  {  onuIpv6HostEntry  10  } 

	onuIpv6HostPrimaryDNS OBJECT-TYPE	
		SYNTAX		OCTET STRING (SIZE(16))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"The manually provisioned IPv6 address of the primary DNS server."
		::=  {  onuIpv6HostEntry  11  } 

	onuIpv6HostSecondaryDNS OBJECT-TYPE	
		SYNTAX		OCTET STRING (SIZE(16))
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"The manually provisioned IPv6 address of the secondary DNS server."
		::=  {  onuIpv6HostEntry  12  } 

	onuIpv6HostOnLinkPrefix OBJECT-TYPE
		SYNTAX		Ipv6Address
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"This attribute is the manually provisioned on-link prefix used for  
                                         destination IPv6 addresses of IPv6 host services."
		::=  {  onuIpv6HostEntry  13  }

	onuIpv6HostOnLinkPrefixLength OBJECT-TYPE
		SYNTAX		Unsigned32
		MAX-ACCESS	read-write
		STATUS		current
		DESCRIPTION	"Prefix length, number of leading bits in the prefix that are valid (1 byte)"
		::=  {  onuIpv6HostEntry  14  } 
 

	onuIpv6HostMacAddress OBJECT-TYPE	
		SYNTAX		MacAddress
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"This attribute indicates the MAC address used by the IP node."
		::=  {  onuIpv6HostEntry  15  } 

	onuIpv6HostIpv6LinkLocalAddress OBJECT-TYPE	
		SYNTAX		Ipv6Address
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"The address used for on-link IP host services, such as router  
                                         solicitation and DHCPv6. [b-IETF RFC 4862] specifies how to automatically 
                                         establish a link-local address."
		::=  {  onuIpv6HostEntry  16  } 

	onuIpv6HostDUID OBJECT-TYPE	
		SYNTAX		OCTET STRING (SIZE(25))
		MAX-ACCESS	read-only 
		STATUS		current
		DESCRIPTION	"This attribute is the DHCPv6 unique identifier. It is an octet string that must  
                                         be globally unique and must remain stable over the lifetime of the ONU. If 
                                         the string is shorter than 25 bytes, it must be null terminated. Its derivation is 
                                         beyond the scope of this recommendation; see [b-IETF RFC 3315] for further 
                                         definition"
		::=  {  onuIpv6HostEntry  17  } 

	onuIpv6HostRowStatus OBJECT-TYPE	
		SYNTAX		RowStatus
		MAX-ACCESS	read-create
		STATUS		current
		DESCRIPTION	"For row creation/deletion."
		::=  {  onuIpv6HostEntry  18  }

--==========================================================================
	onuDot1xTable           OBJECT-TYPE
		SYNTAX          SEQUENCE OF OnuDot1xEntry
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "Parks GPON Dot1x Extension"
		::=  { gponOnuDot1xTable  1 }

	onuDot1xEntry           OBJECT-TYPE
		SYNTAX          OnuDot1xEntry
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "Row Description"
		INDEX           { onuDot1xSlotNo, onuDot1xPortNo,
		                  onuDot1xLogicalPortNo }
		::=  { onuDot1xTable 1 }

	OnuDot1xEntry  ::=  SEQUENCE {
		onuDot1xSlotNo                SlotNo,
		onuDot1xPortNo                PortNo,
		onuDot1xLogicalPortNo         LogicalPortNo,
		onuDot1xProfileIndex          Unsigned32,
		onuDot1xPortBitmap            INTEGER,
		onuPortSecurityProfileIndex   Unsigned32
	}

	onuDot1xSlotNo          OBJECT-TYPE
		SYNTAX          SlotNo
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "Serves as an index for this table."
		::=  { onuDot1xEntry 1 }

	onuDot1xPortNo          OBJECT-TYPE
		SYNTAX          PortNo
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "Serves as an index for this table."
		::=  { onuDot1xEntry 2 }
		
	onuDot1xLogicalPortNo   OBJECT-TYPE
		SYNTAX          LogicalPortNo
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "Serves as an index for this table.
                                 It should be identical to ONU ID assigned by OLT."
		::=  { onuDot1xEntry 3 }

	onuDot1xProfileIndex    OBJECT-TYPE
		SYNTAX          Unsigned32(0..128)
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION     "It points to the associated 802.1x configuration profile index."
		::=  { onuDot1xEntry 4 }

	onuDot1xPortBitmap      OBJECT-TYPE
		SYNTAX          INTEGER
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION     "For ONU 802.1x port configuration."
		DEFVAL          { 0 }
		::=  { onuDot1xEntry 5 }

	onuPortSecurityProfileIndex OBJECT-TYPE
		SYNTAX          Unsigned32(0..1024)
		MAX-ACCESS      read-write
		STATUS          current
		DESCRIPTION     "It points to the associated Port-Security configuration profile index."
		::=  { onuDot1xEntry 6 }

	onuDot1xStatsTable      OBJECT-TYPE
		SYNTAX          SEQUENCE OF OnuDot1xStatsEntry
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "Parks GPON Dot1x Statistics"
		::=  { gponOnuDot1xTable 2 }

	onuDot1xStatsEntry      OBJECT-TYPE
		SYNTAX          OnuDot1xStatsEntry
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "Row Description"
		INDEX           { onuDot1xStatsSlotNo, onuDot1xStatsPortNo,
		                  onuDot1xStatsLogicalPortNo, onuDot1xStatsIndex}
		::=  { onuDot1xStatsTable 1 }
		
	OnuDot1xStatsEntry  ::=  SEQUENCE {
		onuDot1xStatsSlotNo           SlotNo,
		onuDot1xStatsPortNo           PortNo,
		onuDot1xStatsLogicalPortNo    LogicalPortNo,
		onuDot1xStatsIndex            Unsigned32,
		onuDot1xAuthorizedMacAddress  MacAddress,
		onuDot1xConnectedPort         INTEGER,
		onuDot1xConnectedTime         Unsigned32
	}

	onuDot1xStatsSlotNo     OBJECT-TYPE
		SYNTAX          SlotNo
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "Serves as an index for this table."
		::=  { onuDot1xStatsEntry 1 }

	onuDot1xStatsPortNo     OBJECT-TYPE
		SYNTAX          PortNo
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "Serves as an index for this table."
		::=  { onuDot1xStatsEntry 2 }

	onuDot1xStatsLogicalPortNo OBJECT-TYPE
		SYNTAX          LogicalPortNo
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "Serves as an index for this table.
                                 It should be identical to ONU ID assigned by OLT."
		::=  { onuDot1xStatsEntry 3 }

	onuDot1xStatsIndex      OBJECT-TYPE
		SYNTAX          Unsigned32(1..1024)
		MAX-ACCESS      not-accessible
		STATUS          current
		DESCRIPTION     "Serves as an index for this table."
		::=  { onuDot1xStatsEntry 4 }

	onuDot1xAuthorizedMacAddress OBJECT-TYPE
		SYNTAX          MacAddress
		MAX-ACCESS      read-only
		STATUS          current
		DESCRIPTION     "802.1x Authorized MAC Address"
		::=  { onuDot1xStatsEntry 5 }

	onuDot1xConnectedPort   OBJECT-TYPE
		SYNTAX          INTEGER
		MAX-ACCESS      read-only
		STATUS          current
		DESCRIPTION     "LAN Port assigned"
		::=  { onuDot1xStatsEntry 6 }

	onuDot1xConnectedTime   OBJECT-TYPE
		SYNTAX          Unsigned32
		MAX-ACCESS      read-only
		STATUS          current
		DESCRIPTION     "Connected Time measured in time ticks.
                            Count 10ms since the client is connected."
		::=  { onuDot1xStatsEntry 7 }

--==========================================================================
--  OLT SYSTEM MESSAGES --
oltSysMsgTable	OBJECT-TYPE
		SYNTAX		SEQUENCE OF OltSysMsgEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
		           "This table provides system status messages. "
		::=  { gponOltSystemMessages  1 }

oltSysMsgEntry	OBJECT-TYPE
		SYNTAX		OltSysMsgEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Row Description"
		INDEX		{  oltSysMsgSlotNo, oltSysMsgPortNo, oltSysMsgLogicalPortNo }
		::=  { oltSysMsgTable 1 }

OltSysMsgEntry  ::=	SEQUENCE {
		oltSysMsgSlotNo SlotNo,
		oltSysMsgPortNo PortNo,
		oltSysMsgLogicalPortNo LogicalPortNo,
		oltSysMsgCodeBf OCTET STRING,
		oltSysMsgRunTimeCode INTEGER
	}

oltSysMsgSlotNo	OBJECT-TYPE
		SYNTAX		SlotNo
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Serves as an index for this table."
		::=  { oltSysMsgEntry 1 }

oltSysMsgPortNo	OBJECT-TYPE
		SYNTAX		PortNo
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Serves as an index for this table."
		::=  { oltSysMsgEntry 2 }

oltSysMsgLogicalPortNo		OBJECT-TYPE
		SYNTAX		LogicalPortNo
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Serves as an index for this table. It should be identical to 
				ONU ID assigned by OLT. "
		::=  { oltSysMsgEntry 3 }

oltSysMsgCodeBf			OBJECT-TYPE
		SYNTAX		OCTET STRING (SIZE(0..128))
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Bitfield representing codes according to the Dictionary Table."
		::=  { oltSysMsgEntry 4 }

oltSysMsgRunTimeCode		OBJECT-TYPE
		SYNTAX		INTEGER
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Code signed at runtime."
		::=  { oltSysMsgEntry 5 }

--==========================================================================
--  OLT SYSTEM DICTIONARY --
oltSysDicTable			OBJECT-TYPE
		SYNTAX		SEQUENCE OF OltSysDicEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION
		           "This table provides the description for system messages. "
		::=  { gponOltSystemDictionary  1 }

oltSysDicEntry			OBJECT-TYPE
		SYNTAX		OltSysDicEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Row Description"
		INDEX		{  oltSysDicIdx }
		::=  { oltSysDicTable 1 }

OltSysDicEntry  ::=	SEQUENCE {
		oltSysDicIdx Unsigned32,
		oltSysDicFacility SystemModule,
		oltSysDicSeverity INTEGER,
		oltSysDicDesc SysMsgDescription
	}

oltSysDicIdx			OBJECT-TYPE
		SYNTAX		Unsigned32(1..128)
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Serves as an index for this table."
		::=  { oltSysDicEntry 1 }

oltSysDicFacility		OBJECT-TYPE
		SYNTAX		SystemModule
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Module generating the system message."
		::=  { oltSysDicEntry 2 }

oltSysDicSeverity		OBJECT-TYPE
		SYNTAX		INTEGER
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Indicates the message's severity:
				 0  emergency         System unusable
				 1  alert             Immediate action needed
				 2  critical          Critical condition
				 3  error             Error condition
				 4  warning           Warning condition
				 5  notification      Normal but significant condition
				 6  informational     Informational message only
				 7  debugging         Appears during debugging only"
		::=  { oltSysDicEntry 3 }

oltSysDicDesc			OBJECT-TYPE
		SYNTAX		SysMsgDescription
		MAX-ACCESS	read-only
		STATUS		current
		DESCRIPTION	"Message description."
		::=  { oltSysDicEntry 4 }

--==========================================================================
-- OnuCfgTable Extended
	onuCfgExtTable 	OBJECT-TYPE
		SYNTAX		SEQUENCE  OF  OnuCfgExtEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	  "Parks GPON OnuCfgTable Extension "
		::=  { gponOnuCfgExtTable  1 }

	onuCfgExtEntry	OBJECT-TYPE
		SYNTAX		OnuCfgExtEntry
		MAX-ACCESS	not-accessible
		STATUS		current
		DESCRIPTION	"Row Description"
		INDEX		{  onuCfgSlotNo, onuCfgPortNo, onuCfgLogicalPortNo }
		::=  { onuCfgExtTable 1 }
		

	OnuCfgExtEntry  ::=  SEQUENCE {
		onuCfgSlotNo SlotNo,
		onuCfgPortNo PortNo,
		onuCfgLogicalPortNo LogicalPortNo,
		onuCfgIpHostv4Options Unsigned32,
		onuCfgWaitToRestoreTime Unsigned32,
		onuCfgSwitchGuardTime Unsigned32,
		onuCfgRevertiveInd TruthValue,
		onuCfgForceSwitch TruthValue,
		onuCfgOltActiveSlot SlotNo,
		onuCfgOltActivePort PortNo,
